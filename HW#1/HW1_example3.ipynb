   "source": [
    "def check(str1, str2):\n",
    "    \n",
    "    list1 = list(str1)\n",
    "    list2 = list(str2)\n",
    "    \n",
    "    list1.sort()\n",
    "    list2.sort()\n",
    "    \n",
    "    result = list1 == list2\n",
    "   \n",
    "    return result"
   ]
